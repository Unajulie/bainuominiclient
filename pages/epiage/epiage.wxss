.container{
  width: 100%;
  height: auto;
}
form{
  width: 100%;
}
.title{
  width: 100%;
  height: 60px;
  line-height: 60px;
  font-size: 18px;
  font-weight: 600;
  text-align: center;
}
.sub-title{
width:100%;
height: 30px;
line-height: 30px;
font-size: 16px;
font-weight: 600;
text-align: center;
}
.wrap{
  width: 90%;
  height: 50px;
  line-height: 50px;
  margin: 0 auto;
  display: flex;
}

input{
 margin-left: 10px;
  height: 50px;
  line-height: 50px;
}
radio-group{
  display: flex;
  align-items: center;
}
radio-group.radio{
margin-left: 10px;
}
radio-group radio{
  margin-left: 10px;
}
.submitwrap{
  width: 100%;
  height: 40px;
}
.submit{
  width:80%;
  margin-top:130rpx;
  background-color:#036eae;
  color: white;
  border-radius: 30px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;

}