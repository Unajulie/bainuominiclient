.outer{
    width: 95%;
    margin: 0 auto;
    background-color: #fff;
    border-radius: 30rpx;
    margin-bottom: 50rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
}
.outer-last{
    width: 95%;
    margin: 0 auto;
    background-color: #fff;
    border-radius: 30rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    background-color: #eefefb;
}
.section__title{
   font-size: 34rpx;
   font-weight: 600;
   text-align: center;
   margin-top: 30rpx;
}
.body-view{
    width: 100%;
    height: 160rpx;
}
.slider_value{
    font-size: 60rpx;
    font-weight: 700;
    height: 30rpx;
}
.description{
    width: 90%;
    margin: 0 auto;
    height: auto;
    font-size: 32rpx;
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    margin-top: 50rpx;
    padding: 10rpx;
}
.result{
    width: 90%;
    margin: 0 auto;
    font-size: 36rpx;
    font-weight: 600;
    font-family: Arial, Helvetica, sans-serif;
    text-align: center;
    margin-bottom: 50rpx;
}
.val{
font-size: 50rpx;
font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
font-weight: 600;
color: #6ae1c3;
}