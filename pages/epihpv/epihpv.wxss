.container{
  width: 100%;
  height: auto;
}
form{
  width: 100%;
}
.title{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 38rpx;
  font-weight: 600;
  text-align: center;
}
.sub-title{
width:100%;
height: 60rpx;
line-height: 60rpx;
font-size: 32rpx;
font-weight: 600;
text-align: center;
}
.wrap{
  width: 90%;
  height: 100rpx;
  line-height: 100rpx;
  margin: 0 auto;
  display: flex;
}

input{
 margin-left: 20rpx;
  height: 100rpx;
  line-height: 100rpx;
}
radio-group{
  display: flex;
  align-items: center;
}
radio-group.radio{
margin-left: 20rpx;
}
radio-group radio{
  margin-left: 20rpx;
}
.submitwrap{
  width: 100%;
  height: 80rpx;
}
.submit{
  width:80%;
  margin-top:130rpx;
  background-color:#036eae;
  color: white;
  border-radius: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;

}