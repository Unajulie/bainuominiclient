.chech_report{
  text-align: center;
  font-size: 54rpx;
  line-height:1.8;
  color: #ffffff;
  font-weight: 400;
}

.bgepihpv{
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height:300rpx;
  z-index:-1;
}
#check-contain{
  width: 88%;
  height: 100%;
  margin: 0 auto;
}
.privacy{
  width: 100%;
  font-size: 24rpx;
  color: grey;
  text-align: left;
}

.identity{
  width: 100%;
  height: 100rpx;
  font-size: 24rpx;
  line-height: 100rpx;
  align-self: center;
  text-align: center;
  vertical-align: middle;
  border: 2px solid #c477f2;
}
.submit{
  width:50%;
  font-size: 46rpx;
  height: 100rpx;
  margin-top: 80rpx;
  background-color: #c477f2;
  color: white;
  border-radius: 15rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.history{
  width:50%;
  font-size: 46rpx;
  height: 100rpx;
  margin-top: 40rpx;
  background-color: #c7acf8;
  color: white;
  border-radius: 15rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

 /* ---------------------------- */
 /*模态框*/
 .modal{position:fixed; top:0; right:0; bottom:0; left:0; z-index:1000;}
 .modal-cancel{position:absolute; z-index:2000; top:0; right:0; bottom: 0; left:0; background:rgba(0,0,0,0.3);}
 .bottom-dialog-body{width:100%; position:absolute; z-index:3000; bottom:0; left:0;background:#dfdede;}
 /*动画前初始位置*/
 .bottom-positon{-webkit-transform:translateY(100%);transform:translateY(100%);}
  
  
 /* 底部弹出框 */
 .bottom-positon{
  text-align: center;
 }
 .Mselect{
  margin-bottom: 20rpx;
 }
 .Mselect view{
  padding: 26rpx 0;
  background: #fff;
 }
 .Mselect view:not(:last-of-type){
  border-bottom: 1px solid #dfdede;
 }
 .Mcancel{
  background: #fff;
  padding: 26rpx 0;
 }