/* pages/report_epihpv/report_epihpv.wxss */
page{
  width:100%;
  height:100%;
}

  .head{
    position: fixed;
    width: 100%;
    height: 60rpx; 
    line-height: 60rpx;
    background: #fff;
    top: 0;
    left: 0;
  }

.epihpv-wrapper{
  position:relative;
  padding-top: 200rpx;
  padding-bottom: 150rpx;
}
.chech_report{
  text-align: center;
  font-size: 60rpx;
  line-height:60rpx;
  color: #ffffff;
  font-weight: 400;
}
.easy{
  width: 100%;
  text-align: center;
  font-size:36rpx;
  color: #ffffff;
  font-weight: 300;
  line-height:60rpx;

}
.bgepihpv{
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height:300rpx;
  z-index:-1;
}
#check-contain{
  width: 100%;
  height: 100%;
  text-align: center;
}
.privacy{
  width: 100%;
  font-size: 16px;
  color: #916ae5;
  text-align: center;
}

.identity{
  width: 100%;
  height: 100rpx;
  font-size: 18px;
  line-height: 100rpx;
  align-self: center;
  text-align: center;
  vertical-align: middle;
  border: 2rpx solid #916ae5;
}
.submit-wrap{
width: 80%;
height: 80rpx;
display: flex;
align-items: center;
justify-content: center; 
background-color: green;
}
.submit{
  width:80%;
  height: 100rpx;
  margin-top: 80rpx;
  font-size: 38rpx;
  background-color: #916ae5;
  color: white;
  border-radius: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;

}